        "react-helmet-async": "^1.0.9",
        "react-intersection-observer": "^8.32.0",
        "react-lorem-component": "^0.13.0",
        "react-router-dom": "^5.2.0",
        "react-select": "^4.3.1",
        "react-spring": "^9.2.1",
        "recharts": "^2.0.9",
        "styled-system": "5.1.5",
        "stylelint": "^13.13.1",
        "toasted-notes": "3.2.0",
