  "version": "1.2.2",
  "description": "React component library by Audentio",
  "scripts": {
    "test": "jest --passWithNoTests",
    "start": "yarn example:styleguide",
    "example:next": "yarn --cwd ./examples/next dev",
    "example:admin": "EXAMPLE=admin-ui ./node_modules/.bin/webpack serve --config ./webpack/example-dev.js",
    "example:styleguide": "EXAMPLE=styleguide ./node_modules/.bin/webpack serve --config ./webpack/example-dev.js",
    "dev": "EXAMPLE=dev ./node_modules/.bin/webpack serve --config ./webpack/example-dev.js",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook -o storybook-build",
    "storybook:server": "http-server storybook-build",
    "cy": "cypress run",
    "cy:open": "cypress open",
    "build": "rm -rf lib && yarn build:babel && copyfiles src/**/*.png -u 1 ./lib &&  copyfiles src/**/**/*.png -u 1 ./lib",
    "build:tsdecs": "tsc -d --target es5 --emitDeclarationOnly",
    "build:babel": "babel src --out-dir lib --extensions '.ts,.tsx,.js' --source-maps",
    "release": "yarn build && release-it",
    "jest": "./node_modules/.bin/jest",
    "snyk-protect": "snyk protect",
    "prepublishOnly": "npm run build",
    "build-storybook": "build-storybook",
    "editorsdk": "pnpify --sdk vscode"
  },
