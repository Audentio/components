        "tslib": "2.1.0",
        "http-server": "^0.12.1",
        "snyk": "^1.618.0",
        "responsive-loader": "^2.3.0",
        "stylelint-config-audentio": "^1.0.2",
        "stylelint-config-standard": "^22.0.0",
        "typescript": "4.3.2",
        "webpack-cli": "^4.7.0",
        "webpack-dev-server": "^3.11.2"
    },
    "dependencies": {

